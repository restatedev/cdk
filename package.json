{
  "name": "@restatedev/restate-cdk",
  "description": "Restate.dev CDK constructs",
  "version": "1.6.1-SNAPSHOT",
  "author": "Restate Developers",
  "license": "MIT",
  "email": "code@restate.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restatedev/cdk.git"
  },
  "publishConfig": {
    "@restatedev:registry": "https://registry.npmjs.org"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "check": "npm run build && npm run lint && npm run test",
    "prebundle": "rm -rf dist",
    "postbundle": "cd dist && zip -r index.zip index.js*",
    "watch": "tsc -w",
    "test": "jest --config jest.config.js",
    "test:e2e": "npx tsx test/handlers/build.mts && jest --config jest.config.e2e.js",
    "lint": "npx prettier --check .",
    "format": "npx prettier --write .",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.978.0",
    "@restatedev/restate-sdk": "^1.10.2",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/source-map-support": "^0.5.10",
    "esbuild": "^0.27.2",
    "jest": "^29.7.0",
    "jest-cdk-snapshot": "^2.2.5",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.0",
    "zx": "^8.8.5"
  },
  "peerDependencies": {
    "aws-cdk": "^2.215.0",
    "aws-cdk-lib": "^2.215.0",
    "constructs": "^10.3.0",
    "node-fetch": "^3.3.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
